{
  "name": "@krainovsd/react-ui",
  "version": "0.0.1",
  "description": "Krainov react-ui",
  "type": "module",
  "author": "KrainovSD <denislosev48@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/KrainovSD/react-ui"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./tmp/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/bundle.cjs"
    }
  },
  "keywords": [
    "ui-kit",
    "js",
    "react",
    "components"
  ],
  "files": [
    "lib",
    "tmp"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:js": "rollup  --config rollup.config.ts --configPlugin @rollup/plugin-typescript --no-stdin --environment BUILD:production,NODE_ENV:production",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint src/** -c ./eslint.config.js --color --max-warnings 0",
    "lint:debug": "eslint --debug eslint.config.js",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,scss,css,json}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@emotion/css": "11.13.4",
    "@krainovsd/eslint-presets": "0.1.0",
    "@krainovsd/icons": "^0.0.1",
    "@krainovsd/utils": "^0.2.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.1",
    "@storybook/addon-essentials": "8.4.2",
    "@storybook/addon-interactions": "8.4.2",
    "@storybook/blocks": "8.4.2",
    "@storybook/react": "8.4.2",
    "@storybook/react-vite": "8.4.2",
    "@storybook/test": "8.4.2",
    "@tanstack/react-query": "^5.59.20",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "22.8.6",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@types/rollup-plugin-peer-deps-external": "2.2.5",
    "@vitejs/plugin-react": "^4.3.3",
    "antd": "5.21.6",
    "clsx": " 2.1.1",
    "eslint": "9.13.0",
    "i18next": "^23.16.5",
    "lodash": "^4.17.21",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.1.1",
    "rollup": "4.21.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "storybook": "8.4.2",
    "typescript": "5.6.3",
    "vite": "5.4.10"
  },
  "peerDependencies": {
    "@krainovsd/icons": "^0.0.1",
    "clsx": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^15.0.0"
  }
}
